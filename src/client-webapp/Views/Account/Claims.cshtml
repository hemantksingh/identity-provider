@{
	ViewData["Title"] = "My claims";
}
<h2>@ViewData["Title"]</h2>

<table class="table table-striped">
	<thead>
	<tr>
		<th>Claim</th>
		<th>Value</th>
	</tr>
	</thead>
	<tbody>
	@foreach (var claim in User.Claims)
	{
		<tr>
			<td>@claim.Type</td>
			<td>@claim.Value</td>
		</tr>
	}
	</tbody>
</table>